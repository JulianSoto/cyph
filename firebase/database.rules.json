								},
								"timestamp": {
									".validate": "newData.val() === now"
								}
							}
						}
					},

					"unreadMessages": {
						"$unreadMessageSender": {
							"$unreadMessageID": {
								"data": {
									".validate": "newData.val() === ''"
								},
								"hash": {
									".validate": "newData.val() === ''"
								},
								"timestamp": {
									".validate": "newData.val() === now"
								}
							}
						}
					},

					"$other": {
